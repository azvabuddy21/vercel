{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "build": {
    "env": {
      "NEXT_ADD_APP_404_TO_PAGES_MANIFEST": "1"
    }
  },
  "probes": [
    {
      "path": "/",
      "status": 404
    },
    {
      "path": "/admin/foo",
      "status": 404
    },
    {
      "path": "/app-dir/about",
      "status": 200,
      "mustContain": "app router page"
    },
    {
      "path": "/about",
      "status": 200,
      "mustContain": "pages router page"
    }
  ]
}
