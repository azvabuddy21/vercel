{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "build": {
    "env": {
      "NEXT_ADD_APP_404_TO_PAGES_MANIFEST": "1"
    }
  },
  "probes": [
    {
      "path": "/",
      "status": 404,
      "mustContain": "APP ROUTER - 404 PAGE"
    },
    {
      "path": "/admin/foo",
      "status": 404,
      "mustContain": "APP ROUTER - 404 PAGE"
    }
  ]
}
